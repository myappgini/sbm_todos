{{> boxHeader}}
    <ul class="timeline">
        {{#each details}}
        <li>
          <i class="glyphicon glyphicon-user bg-primary"></i>
          <div class="timeline-item">
            <span class="time">
              <i class="glyphicon glyphicon-time"></i>
              <span class="detail-task-time">{{this.date}}</span>
            </span>
            <h4 class="timeline-header description-timeline"> <a href="#">@{{this.user}}</a> <span class="message-timeline">{{this.message}}</span></h4>
          </div>
        </li>
        {{/each}}
    </ul>
{{> boxFooter}}